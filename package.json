{
  "name": "motor-cards-v3",
  "version": "1.0.0",
  "engines": {
    "node": "^8.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "start:hml": "node ./dist/index.js",
    "prestart:dev": "./node_modules/.bin/pm2 delete ecosystem.json; ",
    "start:dev": "cp .env.dev .env && pm2 start --no-daemon -s ecosystem.json",
    "start:dev:stop": "./node_modules/.bin/pm2 delete ecosystem.json",
    "start:dev:kill": "./node_modules/.bin/pm2 kill",
    "start:local": "node dist/index.js",
    "prestart:local": "npm run build",
    "prestart:builded": "npm run build",
    "start:builded": "node ./dist/index.js",
    "prebuild": "rm -rf ./dist",
    "build": "./node_modules/.bin/babel ./src --out-dir dist/ && cp app.yaml ./dist/ && cp package.json ./dist/ && cp -r ./src/swagger ./dist/",
    "build:hml": "./node_modules/.bin/babel ./src --out-dir dist/ && cp app.yaml ./dist/ && cp package.json ./dist/ && cp -r ./src/swagger ./dist/",
    "build:dev": "./node_modules/.bin/babel ./src --out-dir dist/ && cp app.yaml ./dist/ && cp package.json ./dist/ && cp -r ./src/swagger ./dist/",
    "prebuild:test": "rm -rf ./test/build",
    "build:test": "rm -rf ./test/build && ./node_modules/.bin/babel ./test --out-dir ./test/build",
    "test": "npm run build && export DATASTORE_EMULATOR_HOST=localhost:8081 IS_TEST_MODE=1  && npm run build:test && node_modules/.bin/mocha --recursive ./test/build/**/*.test.js && unset IS_TEST_MODE && unset DATASTORE_EMULATOR_HOST",
    "posttest": "rm -rf ./test/build",
    "clean": "rm -f package-lock.json; rm -rf node_modules",
    "clean:build": "rm -rf dist/",
    "lint:src": "eslint src/**/**/*.js src/**/*.js src/*.js",
    "lint": "eslint src/**/**/*.js src/**/*.js src/*.js test/**/**/*.js ",
    "lint:fix": "eslint src/**/**/*.js src/**/*.js src/*.js test/**/**/*.js test/*.js --fix",
    "deploy": "npm run build:hml && cd dist/ && gcloud app deploy app.yaml"
  },
  "dependencies": {
    "@google-cloud/connect-datastore": "^1.0.0",
    "@google-cloud/datastore": "^3.1.2",
    "aws-sdk": "^2.434.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.2",
    "consign": "^0.1.6",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-winston": "^2.4.0",
    "faker": "^4.1.0",
    "helmet": "^3.12.0",
    "moment": "^2.24.0",
    "passport": "^0.4.0",
    "passport-hash": "^0.6.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "swagger-ui-express": "^4.0.1",
    "uuid": "^3.3.2",
    "validatorjs": "^3.14.2",
    "winston": "^3.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-istanbul": "0.12.2",
    "babel-plugin-istanbul": "4.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "6.26.0",
    "browser-sync": "2.24.6",
    "chai": "^4.1.2",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "eslint": "5.2.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "handlebars": "^4.0.12",
    "istanbul": "0.4.5",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.3",
    "pm2": "3.0.3",
    "sinon": "^6.1.5",
    "swagger-client": "^3.8.22"
  }
}
